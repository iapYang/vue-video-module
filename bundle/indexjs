            // requestAnimation id
            requestId: 0,

            // if the video is buffering at the first frame
            is_video_BFF: true,

            // check if is fullscreen
            isFullscreen: false,

            // video
            video: false,

            // check if the current video is ready for play
            videoCanplay: false,

            // Volume temp
            volumeTemp: 1,

            // volume className
            volumeClass: this.options.muted ? 'sound-mute' : 'sound-normal',

            // video options
            videoOptions: Object.assign({
                // source of video (required)
                // src: this.options.src,

                // poster
                poster: false,

                // main play button
                playMain: __webpack_require__(10),
                playMainRollover: __webpack_require__(11),

                // main replay button
                replayMain: __webpack_require__(13),
                replayMainRollover: __webpack_require__(14),

                // sub control button
                playSub: __webpack_require__(9),
                pauseSub: __webpack_require__(8),
                replaySub: __webpack_require__(12),
                fullscreenSub: __webpack_require__(22),
                shrinkscreenSub: __webpack_require__(23),

                // if the video loop
                loop: false,

                // if the video muted
                muted: false,

                // autoPlay
                autoPlay: false,

                // control bar
                controlBar: true,

                // loading
                spinner: 'circles',

                // fullscreen
                fullscreen: false,

                // play inline on isiPhone
                playsinline: true,

                // volumes
                volume: true,

                // when the video can play will call back
                onInit: function onInit() {},


                // video status from pause to play
                onPauseToPlay: function onPauseToPlay() {},


                // video status from play to pause
                onPlayToPause: function onPlayToPause() {},


                // start
                onStart: function onStart() {},


                // end
                onEnded: function onEnded() {},


                // canplay
                onCanplay: function onCanplay() {}
            }, this.options)
        };
    },
    mounted: function mounted() {
        var _this = this;

        this.video = this.$refs.video;
        this.volume = this.$refs.volume;
        this.fullscreenElement = __WEBPACK_IMPORTED_MODULE_0__script_plugin_platform_js___default.a.isDesktop ? this.$refs['vue-video'] : this.video;

        // first time load poster
        this.posterLoadHandler();

        // register events when screen came to whole screen
        var fullscreenEvents = ['fullscreenchange', 'webkitfullscreenchange', 'mozfullscreenchange', 'msfullscreenchange', 'MSFullscreenChange'];

        fullscreenEvents.forEach(function (fullscreenEvent) {
            document.addEventListener(fullscreenEvent, function () {
                _this.checkIsFullscreen();
            });
        });

        // enable inline play
        __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_3_iphone_inline_video__["a" /* default */])(this.video);
    },

    computed: {
        isPlayContainer: function isPlayContainer() {
            return this.videoOptions.playMain && this.videoOptions.playMainRollover;
        },
        isReplayContainer: function isReplayContainer() {
            return this.videoOptions.replayMain && this.videoOptions.replayMainRollover;
        }
    },
    methods: {
        // if the video is ready for play
        canplayHandler: function canplayHandler() {
            var _this2 = this;

            if (!this.count_canplay) {
                this.count_canplay += 1;
                this.videoOptions.onInit(this);
            }
            this.videoCanplay = true;

            this.videoOptions.onCanplay(this);

            // is_video_play = if show the play button
            // in case it'll shown and disappear when auto play
            this.is_video_play = this.videoOptions.autoPlay;

            setTimeout(function () {
                _this2.$refs.volume.reflow();
            }, 100);

            // to auto play
            if (this.videoOptions.autoPlay) {
                setTimeout(this.videoClickHandler, 500);
            }
        },


        // preload poster
        posterLoadHandler: function posterLoadHandler() {
            var _this3 = this;

            // after the poster is loaded, it'll show
            var poster = this.videoOptions.poster;
            if (poster && !this.is_poster_loaded) {
                loadSinglePicture(poster, function () {
                    _this3.is_poster_loaded = true;
                });
            }
        },


        // when close button clicked
        endPointResetHandler: function endPointResetHandler() {
            this.video.currentTime = 0;
            this.progress = '0%';
            this.is_video_play = false;
            this.is_video_played = false;
            this.is_poster_loaded = false;
            this.is_video_BFF = true;
            this.videoCanplay = false;
        },


        // check if video is finished
        checkVideoFinished: function checkVideoFinished() {
            if (this.video.ended) {
                this.showReplay = true;
                this.is_video_play = false;
                this.cancelRequest();
            } else {
                this.showReplay = false;
            }
        },


        // when click the buttons or video itself
        videoClickHandler: function videoClickHandler() {
            if (this.video.paused) {
                this.videoPlayHandler();
            } else {
                this.videoPauseHandler();
            }
        },


        // video pause to play action
        videoPlayHandler: function videoPlayHandler() {
            var _this4 = this;

            this.video.play();
            setTimeout(function () {
                _this4.startRequest();
            }, 100);
            this.videoOptions.onPauseToPlay(this);
            this.is_video_play = true;
        },


        // video play to pause action
        videoPauseHandler: function videoPauseHandler() {
            this.video.pause();
            this.cancelRequest();
            this.videoOptions.onPlayToPause(this);
            this.is_video_play = false;
        },


        // when video is playing
        timeupdateHandler: function timeupdateHandler(e) {
            this.checkVideoFinished();
            this.progress = floatToPercent(e.target.currentTime / e.target.duration);
        },


        // when video is start
        videoStartHandler: function videoStartHandler() {
            if (this.video.currentTime === 0) {
                this.videoOptions.onStart(this);
            }
        },


        // when video is ended
        videoEndedHandler: function videoEndedHandler() {
            this.videoOptions.onEnded(this);
        },


        // when progress bar is clicked
        barClickHandler: function barClickHandler(e) {
            var rate = e.offsetX / e.target.clientWidth;
            this.seek(rate);
        },


        // when click the fullscreen button
        fullscreenClickHandler: function fullscreenClickHandler() {
            if (this.isFullscreen) {
                this.exitFullscreenHandler();
            } else {
                this.requestFullscreenHandler();
            }
        },
        checkIsFullscreen: function checkIsFullscreen() {
            var fullscreenElement = document.fullscreenElement || document.webkitFullscreenElement || document.mozFullScreenElement || document.msFullscreenElement;
            this.isFullscreen = fullscreenElement === this.fullscreenElement;
        },


        // into fullscreen
        requestFullscreenHandler: function requestFullscreenHandler() {
            if (this.fullscreenElement.requestFullscreen) {
                this.fullscreenElement.requestFullscreen();
            } else if (this.fullscreenElement.mozRequestFullScreen) {
                this.fullscreenElement.mozRequestFullScreen();
            } else if (this.fullscreenElement.webkitRequestFullscreen) {
                this.fullscreenElement.webkitRequestFullscreen();
            } else if (this.fullscreenElement.msRequestFullscreen) {
                this.fullscreenElement.msRequestFullscreen();
            } else if (this.fullscreenElement.webkitEnterFullscreen) {
                this.fullscreenElement.webkitEnterFullscreen();
            }
        },


        // exit fullscreen
        exitFullscreenHandler: function exitFullscreenHandler() {
            if (document.exitFullscreen) {
                document.exitFullscreen();
            } else if (document.webkitExitFullscreen) {
                document.webkitExitFullscreen();
            } else if (document.mozCancelFullScreen) {
                document.mozCancelFullScreen();
            } else if (document.msExitFullscreen) {
                document.msExitFullscreen();
            }
        },


        // the request function is to check if the video is buffering, timeupdate is too slow
        // startRequest
        startRequest: function startRequest() {
            var lastTime = -1;

            var self = this;

            function ifVideoBuffered() {
                self.is_video_buffering = lastTime === self.video.currentTime;
                lastTime = self.video.currentTime;
                self.is_video_BFF = self.video.currentTime === 0;

                if (self.video.currentTime !== 0) {
                    self.is_video_played = true;
                }

                var delayTime = 0;

                if (__WEBPACK_IMPORTED_MODULE_0__script_plugin_platform_js___default.a.isFirefox) {
                    delayTime = 40;
                }

                if (__WEBPACK_IMPORTED_MODULE_0__script_plugin_platform_js___default.a.isAndroid) {
                    delayTime = 1000;
                }

                setTimeout(function () {
                    self.requestId = requestAnimationFrame(ifVideoBuffered);
                }, delayTime);
            }

            ifVideoBuffered();
        },


        // cancelRequest
        cancelRequest: function cancelRequest() {
            cancelAnimationFrame(this.requestId);
        },


        // volume mute change
        volumeClickHandler: function volumeClickHandler() {
            if (this.videoOptions.muted) return;

            if (this.video.volume) {
                // case when volume is not 0
                this.volumeTemp = this.video.volume;
                this.volumechangeHandler(1);
                this.volume.muteHandler(1);
            } else {
                if (!this.volumeTemp) this.volumeTemp = 0.1;
                this.volumechangeHandler(1 - this.volumeTemp);
                this.volume.muteHandler(1 - this.volumeTemp);
            }
        },


        // to make the volume change
        // top y = 0, bottom y = 1
        volumechangeHandler: function volumechangeHandler(y) {
            if (!this.video) return;
            this.video.volume = 1 - y;
            this.volumeClassHandler();
        },


        // set class
        volumeClassHandler: function volumeClassHandler() {
            if (this.video.volume >= 0.5) {
                this.volumeClass = 'sound-normal';
            } else if (this.video.volume === 0) {
                this.volumeClass = 'sound-mute';
            } else {
                this.volumeClass = 'sound-low';
            }
        },


        // API
        // video play API
        play: function play() {
            if (this.is_video_play) return;
            this.videoPlayHandler();
        },


        // video pause API
        pause: function pause() {
            if (!this.is_video_play) return;
            this.videoPauseHandler();
        },


        // video seek API
        seek: function seek(rate) {
            this.progress = floatToPercent(rate);
            this.video.currentTime = this.video.duration * rate;
            this.checkVideoFinished();
        },


        // video replay API
        replay: function replay() {
            this.seek(0);
            this.play();
        },


        // change API
        changeVal: function changeVal(val) {
            this.videoOptions = Object.assign({}, this.videoOptions, val);
        },


        // reset
        reset: function reset() {
            this.endPointResetHandler();
        },


        // load poster
        loadPoster: function loadPoster() {
            this.posterLoadHandler();
        }
    },
    props: ['options'],
    components: {
        VueLoading: __WEBPACK_IMPORTED_MODULE_2_vue_simple_loading___default.a,
        VolumeBar: __WEBPACK_IMPORTED_MODULE_1__VueVideo_VolumeBar_vue___default.a
    }
});

