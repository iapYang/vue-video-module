function floatToPercent(number) {
    return number.toFixed(4) * 100 + '%';
}

function loadSinglePicture(src, cb) {
    var downloadingImage = new Image();
    downloadingImage.onload = function () {
        cb();
    };
    downloadingImage.src = src;
